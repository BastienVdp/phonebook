{
    "name": "bastienpp/phonebook",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\": "database/"
        }
    },
    "scripts": {
        "test": [
            "php -d memory_limit=-1 ./vendor/bin/pest"
        ]
    },
    "authors": [
        {
            "name": "BastienVdp",
            "email": "bastienpp@hotmail.com"
        }
    ],
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "pestphp/pest": "^2.24",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "require": {
        "firebase/php-jwt": "^6.9"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
